<body>
  <%- include('header'); -%>
    <section class="u-align-center u-clearfix u-image u-section-8" id="carousel_0188">
        <div class="u-clearfix u-sheet u-valign-middle-lg u-valign-middle-md u-valign-middle-xl u-sheet-1">
            <div class="container">
              <h1> Search by Project Name </h1>
               <form action="/projectlist" method="GET" >
              Enter Project Name: <input type="text" name= "projectname" />
               <input type="submit" value="Search" />
               </form>
               <br >
               <h2>List of the Projects</h2>
               <section data-position='project'>
                   <table class="table table-striped">
                       <thead>
                         <tr>
                           <th scope="col">#</th>
                           <th scope="col">Name</th>
                           <th scope="col">Start Date</th>
                           <th scope="col">End Date</th>
                           <th scope="col">Description Given By User</th>
                           <th scope="col">Assigned To</th>
                           <th scope="col">Feedback Given By Consultant</th>
                           <th scope="col">Action</th>
                         </tr>
                       </thead>
                       <tbody>
                           <% j=1; for ( var i=0; i<project.length; i++) { %>
                         <tr>
                           <th scope="row"><%=[j]%></th>
                           <td><%=project[i].projectname %></td>
                           <td><%=project[i].startdate %></td>
                           <td><%=project[i].enddate %></td>
                           <td><%=project[i].desc %></td>
                           <td><%=project[i].assigned %></td>
                           <td><%=project[i].feedback %> </td>
           
                           <td><a href="/editproject?_id=<%=project[i]._id %>" class="btn btn-info">Edit</a></td>
                           <td><a href="/deleteproject?_id=<%=project[i]._id %>" class="btn btn-danger">Delete</a></td>
                         </tr>
                         <% j++;} %>
                       </tbody>
                     </table>
                   
               </section>
           </div>
              </div>
            </form>
          </div>
        </div>
      </section>
      

