<body>
  <%- include('header'); -%>
    <section class="u-align-center u-clearfix u-image u-section-8" id="carousel_0188">
        <div class="u-clearfix u-sheet u-valign-middle-lg u-valign-middle-md u-valign-middle-xl u-sheet-1">
          <h1 class="u-text u-text-1">Edit Projects </h1>
          <p class="u-text u-text-2">Explore our social media channels to read about Bain's latest insights, our unique culture and our differentiated approach to serving clients and achieving enduring results.<br>
          </p>
          <div class="u-form u-form-1">
        <form action="/editproject" method="Post" class="u-clearfix u-form-spacing-15 u-form-vertical u-inner-form" style="padding: 0;" source="email">
            <div class="u-form-email u-form-group u-form-group-2">
              <label for="projectname" class="u-form-control u-label" wfd-invisible="true">Project Name</label>
              <input type="text" class="form-control" name="projectname" id="projectname" value="<%=project.projectname %>" placeholder="Project Name">
              <input type="hidden" class="form-control" name="id" id="" value="<%=project._id %>" placeholder="Project Name">
            </div>
            <div class="u-form-email u-form-group u-form-group-2">
              <label for="startdate" class="u-form-control u-label" wfd-invisible="true">Start Date</label>
              <input type="text" class="form-control" name="startdate" id="startdate" required value="<%=project.startdate %>"placeholder="">
            </div>
            <div class="u-form-email u-form-group u-form-group-3">
              <label for="endDate" class="u-form-control u-label" wfd-invisible="true">End Date</label>
              <input type="text"  name="enddate" class="form-control" required value="<%=project.enddate %>" id="endDate" placeholder="">
            </div>
            <div class="u-form-email u-form-group u-form-group-4">
              <label for="desc" class="u-form-control u-label" wfd-invisible="true"> Project Description</label>
              <textarea name="desc" class="form-control" required maxlength ="100" id="desc"><%=project.desc %></textarea>
            </div>
            <div class="u-form-email u-form-group u-form-group-5">
              <label for="assigned" class="u-form-control u-label" wfd-invisible="true"> Assigned To</label>
              <textarea name="Assigned" class="form-control"  id="Assigned" placeholder="Written by Consultant"><%=project.Assigned %></textarea>
            </div>
            <div class="u-form-email u-form-group u-form-group-6">
              <label for="feedback" class="u-form-control u-label" wfd-invisible="true"> Project Feedback</label>
              <textarea name="feedback" class="form-control"  id="feedback" placeholder="Written by Consultant"><%=project.feedback %></textarea>
            </div>
            <button type="submit" value="Update" class="btn btn-success ml-2">Update</button>
          </form>
    </div> 
  </div>
</div>
</section>
    </form>
    <script>
      $(document).ready(function() {
        $(function() {
            $( "#startdate" ).datepicker( {
              dateFormat: 'dd/mm/yy',
            })

            $( "#endDate" ).datepicker({
              dateFormat: 'dd/mm/yy',
            })
        });
    })
  </script>
