                <label for="name-" class="u-form-control u-label" wfd-invisible="true">Name</label>
                <input type="text" id="name" required value="<%=consultant.name %>"  name="name" class="u-border-2 u-border-grey-15 u-input u-input-rectangle u-white u-input-1" >
                <input type="hidden" class="form-control" name="id" id="" value="<%=consultant._id %>" >
              </div> 
              <div class="u-form-email u-form-group u-form-group-2">
                <label for="email" class="u-form-control u-label" wfd-invisible="true">Email</label>
                <input type="email"  id="email" name="email"required value="<%=consultant.email %>" class="u-border-2 u-border-grey-15 u-input u-input-rectangle u-white u-input-2" >
              </div>
              <div class="u-form-email u-form-group u-form-group-2">
                <label for="qualification" class="u-form-control u-label" wfd-invisible="true">Qualifications</label>
                <textarea type="text"  id="qualification" name="qualification" class="u-border-2 u-border-grey-15 u-input u-input-rectangle u-white u-input-2" ><%=consultant.qualification %> </textarea>
              </div>             
              <button type="submit" value="Update" class="btn btn-success ml-2">Update</button>
              </div>
            </form>
          </div>
        </div>
